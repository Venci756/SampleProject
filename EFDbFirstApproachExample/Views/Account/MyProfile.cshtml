@model EFDbFirstApproachExample.Identity.ApplicationUser
@{
    ViewBag.Title = "MyProfile";
}

<h2>MyProfile</h2>
<table class="table">
    <tr>
        <th>Username</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Phone</th>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <th>Date of birth</th>
        <td>@Model.Birthday</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>
</table>

<a href="/Home/Index">Back</a>
